        print("{0} í˜¸ ".format(apart[i][j]))




#300
#try, except, else, finally
per = ["10.31", "", "8.00"]
try:
    for i in per:
        print(float(per))
except TypeError as e:
    print(e)
else:
    print("Everything is fine")
finally:
    print("Cleaning up...")





class Animals():
    def __init__(self, name, age, food):
        self.name = name
        self.age = age
        self.food = food
    def bark(self):
        if self.age <3:
            print("Wooh Wooh Wooh")
        else:
            print("WOOF WOOF WOOF")
    
    def mew(self):
        if self.age <3:
            print("mewww, mewww")
        else:
            print("MEWWW, MEWWW")

dog = Animals("Rex", 5, "Meat")
dog.bark()

cat = Animals("Nany", 3, "Milk")
cat.mew()
